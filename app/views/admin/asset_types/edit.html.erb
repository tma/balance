    <%= ui_breadcrumb_item "Asset Types", admin_asset_types_path %>
    <%= ui_breadcrumb_item @asset_type.name, admin_asset_type_path(@asset_type) %>
  <% end %>
<% end %>

<div>
  <div class="flex items-center h-10 mb-6">
    <h1 class="<%= ui_title_class %>">Edit Asset Type</h1>
